  "homepage": "https://www.presstify.com/pollen-solutions/log/",
  "keywords": [
    "pollen-solutions",
    "component",
    "logs",
    "logging",
    "PSR-3"
  ],
  "time": "2021-08-12",
  "license": "MIT",
